{
  "compilerOptions": {
    "baseUrl": "./",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "es6",
    "paths": {
      "#api/*": [
        "./src/api/*"
      ],
      "#configs/*": [
        "./src/configs/*"
      ],
      "#shares/*": [
        "./src/shares/*"
      ],
      "#services/*": [
        "./src/services/*"
      ]
    }
  }
}
